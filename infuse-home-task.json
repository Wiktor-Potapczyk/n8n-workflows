    {
      "parameters": {
        "jsCode": "let allItems = [];\n\nfor (const item of items) {\n  const json = item.json;\n\n  // --- CASE 1: Reddit (Targeted Subreddits RSS) ---\n  if (json.feed && json.feed.entry) {\n    const entries = Array.isArray(json.feed.entry) ? json.feed.entry : [json.feed.entry];\n    \n    for (const entry of entries) {\n      const link = entry.link?.href || (Array.isArray(entry.link) ? entry.link[0]?.href : '') || '';\n      // WyciÄ…gamy nazwÄ™ subreddita z kategorii, np. \"r/SaaS\"\n      const subreddit = entry.category?.label || 'Reddit'; \n      const contentParams = (entry.title || '') + '\\n' + (entry.content?._ || '');\n\n      allItems.push({\n        json: {\n          id: entry.id, \n          platform: `Reddit (${subreddit})`, // Np. \"Reddit (r/SaaS)\"\n          author: entry.author?.name || 'Unknown',\n          content: contentParams,\n          link: link,\n          date: entry.updated || entry.published || new Date().toISOString()\n        }\n      });\n    }\n  }\n\n  // --- CASE 2: LinkedIn (Apify Sync) ---\n  else if (json.activity_id) {\n    allItems.push({\n      json: {\n        id: json.activity_id,\n        platform: 'LinkedIn', \n        author: json.author?.name || 'Unknown',\n        content: json.text || json.content?.title || 'No text content',\n        link: json.post_url,\n        date: json.posted_at?.date || new Date().toISOString()\n      }\n    });\n  }\n}\n\nreturn allItems;"
      },
